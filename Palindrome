//Enter a string
If string is palindrome then print Yes else No
 
 
 string str = Console.ReadLine();
		//string str = "1221";
		string revstr = null;
		for (int i = str.Length-1; i>=0;i--)
	    {
	        revstr = revstr + str[i];
	    }
	    if(str == revstr)
	    {
	       	Console.WriteLine("Yes");
	    }
	    else
	       	Console.WriteLine("No");
