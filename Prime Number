a number divide any other number and modules is 0 is not a prime number.
prime number is divided by 1 or self number.

main()
{
           int num = Convert.ToInt32(Console.ReadLine());
            if (isPrime(num))
            {
                Console.WriteLine(num + " is Prime ");
            }
            else
                Console.WriteLine(num + " is not Prime ");
}

        static Boolean isPrime(int num)
        {
            if (num <= 1)
            {
                return false;
            }
            for (int i = 2; i < num; i++)
            {
                if (num % i == 0)
                    return false;
            }
            return true;
        }
